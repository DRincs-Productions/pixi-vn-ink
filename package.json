{
  "name": "@drincs/pixi-vn-ink",
  "version": "0.9.6",
  "type": "module",
  "description": "Pixi'VN gives you the ability to write your own narrative using Ink",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs",
      "default": "./dist/vite.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "vite": ["./dist/vite.d.ts"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DRincs-Productions/pixi-vn-ink.git"
  },
  "author": "DRincs-Productions",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DRincs-Productions/pixi-vn-ink/issues"
  },
  "homepage": "https://pixi-vn.web.app/ink",
  "funding": "https://github.com/DRincs-Productions/pixi-vn?sponsor=1",
  "dependencies": {
    "@drincs/pixi-vn-json": "^1.7.7"
  },
  "devDependencies": {
    "inkjs": "^2.3.2",
    "jsdom": "^26.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite.js": "^1.0.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@drincs/pixi-vn": ">=1.3.19"
  },
  "keywords": [
    "game",
    "ink",
    "js",
    "novel",
    "pixi",
    "pixi-js",
    "pixi-vn",
    "visual",
    "visual-novel",
    "vn"
  ]
}
